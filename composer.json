{
	"name": "noend/profitshare-client",
	"type": "library",
	"description": "Profitshare modern client to consume the profitshare API.",
	"keywords": [
		"profitshare",
		"api"
	],
	"homepage": "https://github.com/noend/profitshare-client",
	"license": "MIT",
	"authors": [
		{
			"name": "Catalin Ionut Titov",
			"email": "catalin.titov@gmail.com",
			"homepage": "https://titov.ro/",
			"role": "PHP Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"ProfitshareClient\\": "src/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": ">=7.4",
		"ext-curl": "*",
		"ext-json": "*",
		"netresearch/jsonmapper": "^4.0",
		"symfony/http-client": "^5.1"
	},
	"scripts": {
		"analyse": "vendor/bin/phpstan analyse -c phpstan.neon"
	},
	"require-dev": {
		"phpstan/phpstan": "^1.2.0"
	}
}
